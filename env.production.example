# Production Environment Variables
# Copy this file to .env.production and fill in your production values
# 
# ⚠️ IMPORTANT: Never commit actual production keys to version control!
# These are example values - replace with your actual production credentials

# Production Supabase Project URL
# Use your production Supabase project URL
VITE_SUPABASE_URL=https://ltjxhzfacfqfxkwzeinc.supabase.co

# Production Supabase Publishable/Anon Key
# Get this from: Supabase Dashboard → Settings → API → Project API keys → anon public
# This is safe to use in the frontend
VITE_SUPABASE_ANON_KEY=your_production_anon_key_here

# Production Site URL (for redirects, CORS, etc.)
# Update this with your actual production domain
VITE_SITE_URL=https://yourdomain.com

# Environment identifier
VITE_ENV=production

# Enable/disable features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true

